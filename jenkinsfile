pipeline {
    agent any
    tools {
         jdk "java-11"
        }
    stages {
        stage('Build') {
            steps {
                bat 'javac JavaExample.java'
                bat 'java JavaExample'
               }
         }
         stage('Test') {
            steps {
                echo "Testing"
                }
           }
         stage('Package') {
             steps {
                echo "Created Package"
                }
           }
       }
    }
         
